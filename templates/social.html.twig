<div class="widget HTML">
    <h2 class="title">{{ module.title }}</h2>
    <div class="widget-content">
        {% for item in module.header.social %}
            <a class="social-shortcode" href="{{ item.url }}">{{ module.media.images[item.icon]|raw }}</a>
        {% endfor %}
        {{ module.content|raw }}
    </div>
</div>
